

<%= form_for @contact, method: :put do |f| %>
<%= f.label :first_name %>
<%= f.text_field :first_name%><br />
<%= f.label :last_name %>
<%= f.text_field :last_name %><br />
<%= f.label :email %>
<%= f.text_field :email%><br />
<%= f.label :phone %>
<%= f.text_field :phone%><br />
<%= f.label :note%>
<%= f.text_area :note%>
<%= f.submit "UPDATE" %>

  <% end %>
