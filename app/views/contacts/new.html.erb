<h1>Create a new contact</h1>

<%= form_for @contact do |f| %>
<%= f.label :first_name %>
<%= f.text_field :first_name%><br />
<%= f.label :last_name %>
<%= f.text_field :last_name %><br />
<%= f.label :email %>
<%= f.text_field :email%><br />
<%= f.label :phone %>
<%= f.text_field :phone%><br />
<%= f.label :note%>
<%= f.text_area :note%>
<%= f.submit "SAVE" %>

  <% end %>
