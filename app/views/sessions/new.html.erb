<h1 id="main_title">Login</h1>
<a href="/signup"><p id="signin">Not a user? Sign up here!</p></a>
<p id="alert"><%= flash[:alert] %></p>

<%= form_tag '/login' do %>
  <p>Email</p>
  <%= text_field_tag :email %>
  <p>Password</p>
  <%= password_field_tag :password %>
  <%= submit_tag "Submit" %>

<% end %>
